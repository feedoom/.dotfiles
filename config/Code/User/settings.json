{
  "files.autoSave": "off",
  "window.zoomLevel": -1,

  "editor.renameOnType": true,
  "editor.cursorSurroundingLines": 7,
  "editor.lineNumbers": "relative",
  "editor.wordWrap": "on",
  "editor.fontSize": 20,
  "editor.fontFamily": "'Source Code Pro'",
  // "editor.fontFamily": "'monospace'",
  "editor.tabCompletion": "on",
  "editor.quickSuggestions": true,
  "editor.parameterHints.enabled": true,
  "editor.smoothScrolling": true,
  "editor.suggestSelection": "none",

  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,

  "workbench.statusBar.visible": true,
  "workbench.activityBar.visible": true,
  "workbench.colorTheme": "One Dark Pro",

  // emmet
  "emmet.triggerExpansionOnTab": true,
  "emmet.includeLanguages": {
    "javascript": "html"
  },

  "terminal.integrated.fontFamily": "'Source Code Pro Medium'",
  //"terminal.integrated.shell.linux": "/usr/bin/zsh",
  "terminal.integrated.shell.windows": "C:\\Windows\\System32\\wsl.exe",

  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "java.semanticHighlighting.enabled": true,

  //不要高亮相同的单词
  "editor.occurrencesHighlight": false,

  //右边的导航大纲
  "editor.minimap.enabled": false,

  //导航路径
  "breadcrumbs.enabled": true,

  //空格可见
  //"editor.renderWhitespace": "all",

  //文件末尾另起一行
  "files.insertFinalNewline": true,

  //剪裁尾部空格
  "files.trimTrailingWhitespace": true,

  //忽略文件
  "files.exclude": {
    "**/.history": true,
    "**/node_modules": true
  },

  "latex-workshop.view.pdf.viewer": "external",

  //自动格式化
  "editor.formatOnType": true,
  "editor.formatOnSave": true,

  //图标
  "workbench.iconTheme": "vscode-great-icons",

  //clipboard manager
  "clipboard-manager.maxClips": 100,
  "clipboard-manager.preview": false,
  "clipboard-manager.snippet.enabled": false,

  //liveServer
  "liveServer.settings.donotShowInfoMsg": true,

  //leetcode
  "leetcode.endpoint": "leetcode-cn",

  "translator.translateOnHover": true,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "cSpell.userWords": ["lsdjfslf"],

  //todo tree
  "todo-tree.tree.showScanModeButton": false,

  //sync
  "sync.gist": "142e96bf6cf128e420a00d0c51bb87df",
  "sync.autoDownload": false,
  "sync.autoUpload": false,
  "sync.quietSync": true,
  "sync.forceUpload": true,

  //eslint
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },

  //vim
  // vim输入法
  "vim.timeout": 2000,
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false,
    "<C-c>": true
  },
  "vim.highlightedyank.enable": true,
  "vim.autoSwitchInputMethod.enable": true,
  "vim.autoSwitchInputMethod.defaultIM": "1033",
  "vim.autoSwitchInputMethod.obtainIMCmd": "D:\\bin\\im-select.exe",
  "vim.autoSwitchInputMethod.switchIMCmd": "D:\\bin\\im-select.exe {im}",
  //"vim.autoSwitchInputMethod.defaultIM": "1",
  //"vim.autoSwitchInputMethod.obtainIMCmd": "/usr/bin/fcitx-remote",
  //"vim.autoSwitchInputMethod.switchIMCmd": "/usr/bin/fcitx-remote -t {im}",
  //"vim.statusBarColorControl": false,
  "vim.easymotion": true,
  "vim.sneak": true,
  "vim.incsearch": false,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.searchHighlightColor": "blue",
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    },
    {
      "before": ["<C-j>"],
      "after": ["<down>"]
    },
    {
      "before": ["<C-k>"],
      "after": ["<up>"]
    },
    {
      "before": ["<C-l>"],
      "after": ["<right>"]
    },
    {
      "before": ["<C-h>"],
      "after": ["<Left>"]
    },
    {
      "before": ["<C-u>"],
      "after": ["<Esc>", "b", "~", "e", "a"]
    },
    {
      "before": ["<a-u>"],
      "commands": ["editor.action.transformToUppercase"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["j"],
      "after": ["g", "j"]
    },
    {
      "before": ["k"],
      "after": ["g", "k"]
    },
    {
      "before": ["g", "j"],
      "after": ["j"]
    },
    {
      "before": ["g", "k"],
      "after": ["k"]
    },
    {
      "before": ["<leader>", "n"],
      "commands": [":nohl"]
    },
    {
      "before": ["<C-j>"],
      "after": ["j", "j", "j", "j", "j"]
    },
    {
      "before": ["<C-k>"],
      "after": ["k", "k", "k", "k", "k"]
    },
    {
      "before": ["W"],
      "after": [":", "w", "<Enter>"]
    },
    {
      "before": ["Q"],
      "after": [":", "q", "<Enter>"]
    },
    {
      "before": ["L"],
      "after": ["g", "_"]
    },
    {
      "before": ["H"],
      "after": ["^"]
    },
    {
      "before": ["E"],
      "after": ["g", "T"]
    },
    {
      "before": ["R"],
      "after": ["g", "t"]
    },
    {
      "before": [">"],
      "after": [">", ">"]
    },
    {
      "before": ["<"],
      "after": ["<", "<"]
    },
    {
      "before": ["g", "v"],
      "after": ["`", "[", "v", "`", "]"]
    },
    {
      "before": ["<leader>", "v"],
      "after": ["V", "`", "}"]
    },
    {
      "before": ["<leader>", "/"],
      "after": ["g", "c", "c"]
    },
    {
      "before": ["f", "f"],
      "after": ["<leader>", "<leader>", "s"]
    },
    {
      "before": ["U"],
      "after": ["<C-r>"]
    },
    {
      //添加书签
      "before": ["<leader>", "m"],
      "commands": ["bookmarks.toggle"]
    },
    {
      "before": ["<leader>", "b"],
      "commands": ["bookmarks.list"]
    },
    {
      //重命名函数
      "before": ["<leader>", "r", "n"],
      "commands": ["editor.action.rename"]
    },
    {
      //在右侧打开定义
      "before": ["g", "p"],
      "commands": ["editor.action.revealDefinitionAside"]
    },
    {
      "before": [":"],
      "commands": ["workbench.action.showCommands"]
    },
    {
      "before": ["[", "b"],
      "commands": ["workbench.action.previousEditor"]
    },
    {
      "before": ["]", "b"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["K"],
      "commands": ["editor.action.showHover"]
    },
    {
      "before": ["<leader>", "f", "S"],
      "commands": ["workbench.action.files.saveAll"]
    },
    {
      "before": ["<leader>", "g", "y"],
      "commands": ["workbench.action.toggleZenMode"]
    },
    {
      "before": ["t", "r"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },
    {
      "before": ["<leader>", "p"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      //格式化
      "before": ["<leader>", "f", "o"],
      "commands": ["editor.action.formatDocument"]
    },
    {
      //驼峰命名
      "before": ["<leader>", "r"],
      "commands": ["extension.changeCase.camel"]
    },
    {
      //添加空行
      "before": ["]", "<leader>"],
      "commands": ["editor.action.insertLineAfter"]
    },
    {
      "before": ["<leader>", "c"],
      "commands": ["notifications.clearAll"]
    },
    {
      "before": ["<leader>", "t"],
      "commands": ["extension.翻译选中文本"]
    },
    {
      "before": ["[", "<leader>"],
      "commands": ["editor.action.insertLineBefore"]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["<C-j>"],
      "after": ["j", "j", "j", "j", "j"]
    },
    {
      "before": ["<C-k>"],
      "after": ["k", "k", "k", "k", "k"]
    },
    {
      "before": ["H"],
      "after": ["^"]
    },
    {
      "before": ["L"],
      "after": ["g", "_"]
    },
    {
      "before": ["<c-l>"],
      "commands": ["editor.action.moveCarretRightAction"]
    },
    {
      "before": ["<c-h>"],
      "commands": ["editor.action.moveCarretLeftAction"]
    },
    {
      "before": ["<leader>", "f", "o"],
      "commands": ["editor.action.formatDocument"]
    },
    {
      "before": ["Y"],
      "after": ["\"", "+", "y"]
    },
    {
      "before": [">"],
      "after": [">", "g", "v"]
    },
    {
      "before": ["<"],
      "after": ["<", "g", "v"]
    },
    {
      "before": ["<leader>", "c"],
      "commands": ["notifications.clearAll"]
    },
    {
      "before": ["<leader>", "t"],
      "commands": ["extension.翻译选中文本"]
    },
    {
      //注释
      "before": ["<leader>", "/"],
      "after": ["g", "c"]
    },
    {
      "before": [":"],
      "commands": ["workbench.action.showCommands"]
    },
    {
      //驼峰命名
      "before": ["<leader>", "r"],
      "commands": ["extension.changeCase.camel"]
    },
    {
      //对齐
      "before": ["<leader>", "a"],
      "commands": ["wwm.aligncode"]
    }
  ],
  "[javascript]": {
    // "editor.defaultFormatter": "vscode.typescript-language-features",
    //prettier
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    // "editor.defaultFormatter": "vscode.typescript-language-features",
    //prettier
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "latex-workshop.latex.recipes": [
    {
      "name": "xelatex",
      "tools": ["xelatex"]
    },
    {
      "name": "xelatex->bibtex->exlatex*2",
      "tools": ["xelatex", "bibtex", "xelatex", "xelatex"]
    }
  ],
  "latex-workshop.latex.tools": [
    {
      "name": "xelatex",
      "command": "xelatex",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "%DOC%"
      ]
    },
    {
      "name": "bibtex",
      "command": "bibtex",
      "args": ["%DOCFILE%"]
    }
  ]
}
